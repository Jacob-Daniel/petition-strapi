{
	"kind": "collectionType",
	"collectionName": "navigation_items",
	"info": {
		"singularName": "navigation-item",
		"pluralName": "navigation-items",
		"displayName": "Navigation Item",
		"description": ""
	},
	"options": {
		"draftAndPublish": true
	},
	"attributes": {
		"slug": {
			"type": "uid"
		},
		"order": {
			"type": "integer"
		},
		"target": {
			"type": "enumeration",
			"enum": ["_self", "_blank"],
			"default": "_self"
		},
		"active": {
			"type": "boolean",
			"default": true
		},
		"navigation": {
			"type": "relation",
			"relation": "manyToMany",
			"target": "api::navigation.navigation",
			"inversedBy": "navigation_items"
		},
		"is_parent": {
			"type": "boolean",
			"default": false
		},
		"children": {
			"type": "relation",
			"relation": "oneToMany",
			"target": "api::navigation-item.navigation-item",
			"mappedBy": "parent"
		},
		"parent": {
			"type": "relation",
			"relation": "manyToOne",
			"target": "api::navigation-item.navigation-item",
			"inversedBy": "children"
		},
		"label": {
			"type": "string"
		}
	}
}
