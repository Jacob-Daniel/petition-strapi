{
  "kind": "collectionType",
  "collectionName": "navigation_items",
  "info": {
    "singularName": "navigation-item",
    "pluralName": "navigation-items",
    "displayName": "Navigation Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "order": {
      "type": "integer"
    },
    "target": {
      "type": "enumeration",
      "default": "_self",
      "enum": [
        "_self",
        "_blank"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "navigation": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::navigation.navigation",
      "inversedBy": "navigation_items"
    },
    "is_parent": {
      "type": "boolean",
      "default": false
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::navigation-item.navigation-item",
      "mappedBy": "parent"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::navigation-item.navigation-item",
      "inversedBy": "children"
    },
    "label": {
      "type": "string"
    },
    "fragment": {
      "type": "string"
    },
    "is_button": {
      "type": "boolean",
      "default": false
    }
  }
}
